{
  "expo": {
    "name": "TimeTOGO",
    "slug": "timetogo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/TimeTOGO-icon.png",
    "scheme": "timetogo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-TimeTOGO-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.timetogo.timetogo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "TimeTOGO - 시간 맞춤 경로 안내",
      "shortName": "TimeTOGO",
      "lang": "ko",
      "scope": "/",
      "themeColor": "#3457D5",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f7b83280-3634-4618-b359-a205ca0b51db"
      }
    },
    "owner": "m2049"
  }
}
